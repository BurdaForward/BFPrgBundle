{
    "name": "burdaforward/bf-prg-bundle",
    "description": "This bundle helps you to implement the PRG pattern into your application.",
    "type": "symfony-bundle",
    "version": "1.2.2",
    "license": "MIT",
    "authors": [
        {
            "name": "Thilo Habenreich",
            "email": "thabenreich@chip.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^5.4 || ^6.4",
        "twig/twig": "^2.15 || ^3.0",
        "symfony/yaml": "^5.4 || ^6.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "BurdaForward\\BFPrgBundle\\": "/src"
        }
    }
}
